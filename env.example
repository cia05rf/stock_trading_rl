# =============================================================================
# Intraday Price Fetch - Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control

# =============================================================================
# API CONFIGURATION
# =============================================================================

# EODHD API Key (required for data fetching)
# Get your key at: https://eodhd.com/
API_KEY=your_api_key_here

# Data interval for price fetching (e.g., "15m", "1h", "1d")
INTERVAL=15m

# =============================================================================
# MODE CONFIGURATION
# =============================================================================

# Enable test mode (limits data fetching for testing)
TEST_MODE=false

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# =============================================================================
# PATHS (optional - defaults in config.json)
# =============================================================================

# Override data directory
# DATA_DIR=./data

# Override HDF5 file path
# HDF5_FILE=./data/prices_highly_liquid.h5

# =============================================================================
# TRAINING CONFIGURATION (optional overrides)
# =============================================================================

# Device for PyTorch (cuda, cpu, mps)
# DEVICE=cuda

# Random seed for reproducibility
# SEED=42

# Initial balance for trading environment
# INITIAL_BALANCE=10000

# Total training timesteps
# TOTAL_TIMESTEPS=2000000

# Number of training epochs
# EPOCHS=5

